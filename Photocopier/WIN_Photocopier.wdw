     code : |1-
      // Summary: Initializes the window
      // Syntax:
      //InitializeWindow ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE InitializeWindow()
      
      
      // Default print color for the photocopy
      COMBO_ColorimetricMode = LoadParameter(PARAM_COLORMODE, TwainBlackWhite)
      
      // Initialize the list of printers available on the computer
      ListDeleteAll(COMBO_Destination)
      WHEN EXCEPTION IN 
      	
      	// Add the list of printers available on the computer
      	ListAdd(COMBO_Destination, iListPrinter())
      	
      	// If no printer is found
      	IF COMBO_Destination..Occurrence = 0 THEN
      		NoPrinter()
      	ELSE // If at least one printer is found
      		
      		// Retrieve the characteristics of the default printer
      		let sDefaultPrinter = LoadParameter(PARAM_PRINTER, iInfoPrinter(False))
      		// Find the default printer in the list
      		let nPrinterSubscript = ListSeek(COMBO_Destination, sDefaultPrinter, True)
      		// If the printer is not found, selects the first printer in the list
      		IF nPrinterSubscript <= -1 THEN nPrinterSubscript = 1
      		// Select the printer
      		ListSelectPlus(COMBO_Destination, nPrinterSubscript)
      		
      	END
      	
      DO
      	NoPrinter()
      END
      
      	INTERNAL PROCEDURE NoPrinter()
      	
      	// Unable to photocopy: no printer is available
      	Info(<$0007>)
      	BTN_Photocopying..State = Grayed
      	
      	END
      
      
      // Initialize the list of Twain devices available on the computer
      ListDeleteAll(COMBO_Source_)
      WHEN EXCEPTION IN 
      	
      	// Add the list of Twain devices available on the computer
      	ListAdd(COMBO_Source_, TwainListSource())
      	// If no device is found
      	IF COMBO_Source_..Occurrence = 0 THEN
      		NoTwainDevice()
      	ELSE // If at least one device is found
      		
      		// Search for the scanner name saved in the registry:
      		let sDefaultTwain = LoadParameter(PARAM_TWAIN, "")
      		// Find the default device in the list
      		let nTwainSubscript = ListSeek(COMBO_Source_, sDefaultTwain, True)
      		// If the device is not found, selects the first device in the list
      		IF nTwainSubscript <= -1 THEN nTwainSubscript = 1
      		// Select the device
      		ListSelectPlus(COMBO_Source_, nTwainSubscript)
      		
      	END
      	
      DO
      	NoTwainDevice()
      END
      
      	INTERNAL PROCEDURE NoTwainDevice()
      	
      	// The photocopy cannot be performed: no available Twain device
      	Info(<$0002>)
      	BTN_Photocopying..State = Grayed
      	
      	END
      
      // Initialize the lightness
      SLD_Lightness = LoadParameter(PARAM_LIGHTNESS, 0)
      EDT_LightnessValue = SLD_Lightness
      
      // Display the scale parameter
      EDT_Scale = LoadParameter(PARAM_SCALE, 100)
      
      // Parameters by mode
      // - black and white
      LooperAddLine(LOOP_Parameters, TwainBlackWhite, <$000f>, LoadParameter(PARAM_RESOLUTION_BW, 300), LoadParameter(PARAM_PRINTER_BW, iInfoPrinter(False, iPrinterName)))
      // - grayscale
      LooperAddLine(LOOP_Parameters, TwainGrayScale, <$0010>, LoadParameter(PARAM_RESOLUTION_GS, 150), LoadParameter(PARAM_PRINTER_GS, iInfoPrinter(False, iPrinterName)))
      // - color
      LooperAddLine(LOOP_Parameters, TwainRGB, <$0011>, LoadParameter(PARAM_RESOLUTION_COLOR, 150), LoadParameter(PARAM_PRINTER_COLOR, iInfoPrinter(False, iPrinterName)))
      
      // 1 copy by default
      EDT_NbCopies = 1
