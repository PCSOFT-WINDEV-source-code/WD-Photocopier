         -
           internal_properties : CAAAAAgAAABlEY0Q6zoY9dtcud94PLLt0mQm9I8EHrm7MOqEUOfrnzhNgYE23UtPDNfkrd8iQdaXCwE/wkO4w7qDg8xVkR9+SiYAugGGuOKim2XUjEwvRkDtlPgK5kjvmpvmErawa22SzrnBgxaWHa/BYBu+T6X51Ht5H/mkqYVQudjt+ebqaQ==
           code : |1+
            // Confirm
            IF Dialog(<$0000>, [<$000e>, <$000d>], 1, 2, dlgIconQuestion) = 2 THEN RETURN
            
            // Twain device
            COMBO_Source_ = TwainCurrentSource()
            SaveParameter(PARAM_TWAIN, COMBO_Source_)
            
            // Retrieve the characteristics of the default printer
            let sDefaultPrinterName = iInfoPrinter(False, iPrinterName)
            // Printer
            COMBO_Destination = sDefaultPrinterName
            SaveParameter(PARAM_PRINTER, COMBO_Destination)
            // Printer for the images in black&white
            LOOP_Parameters.ATT_Printer[1] = sDefaultPrinterName
            SaveParameter(PARAM_PRINTER_BW, sDefaultPrinterName)
            // Printer for the images in grayscale
            LOOP_Parameters.ATT_Printer[2] = sDefaultPrinterName
            SaveParameter(PARAM_PRINTER_GS, sDefaultPrinterName)
            // Printer for the images in color
            LOOP_Parameters.ATT_Printer[3] = sDefaultPrinterName
