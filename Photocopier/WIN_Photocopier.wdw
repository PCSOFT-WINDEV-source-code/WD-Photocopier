     p_codes :
      -
        internal_properties : CAAAAAgAAADfmY4FLfJ8Qhuwy57Yo7AZ38E0oBgR8wfj3i9kVIGHFxVYzmx7hKlRrL8PwNTilQ5jXqLsgFg4+oCPdlvKZiQGb0faCWc6xqa20ED6ALieLK3/flF08RSFQlIjqqnfbVZH61XHBBf+vrACI3g=
        code : |1-
         // Retrieve the parameters defined for the photocopy
         pGlobal.gstPhotocopy:nNbCopy = EDT_NbCopies // Number of copies
         pGlobal.gstPhotocopy:bPreview = BTN_Preview..Pushed // Is a print preview required? 
         pGlobal.gstPhotocopy:nScale = EDT_Scale // Scale 
         
         // If no printer is selected
         IF ListSelect(COMBO_Destination) <= -1 THEN ReturnToCapture(COMBO_Destination)
         // Stores the selected printer
         pGlobal.gstPhotocopy:sPrinter = COMBO_Destination[ListSelect(COMBO_Destination)]
         
         // Modifies the destination printer
         iConfigure(pGlobal.gstPhotocopy:sPrinter, True)
         
         // If no device is selected
         IF ListSelect(COMBO_Source_) <= -1 THEN ReturnToCapture(COMBO_Source_)
         // Stores the selected device
         pGlobal.gstPhotocopy:sScan = COMBO_Source_[ListSelect(COMBO_Source_)]
         
         // Retrieve the selected color and the resolution
         SWITCH COMBO_ColorimetricMode
         	// Black and white
         	CASE TwainBlackWhite
         		pGlobal.gstPhotocopy:nColor = TwainBlackWhite
         		pGlobal.gstPhotocopy:nResolution = LoadParameter(PARAM_RESOLUTION_BW, 300)
         		pGlobal.gstPhotocopy:nNbBit = 1
         		
         	// Grayscale
         	CASE TwainGrayScale
         		pGlobal.gstPhotocopy:nColor = TwainGrayScale
         		pGlobal.gstPhotocopy:nResolution = LoadParameter(PARAM_RESOLUTION_GS, 150)
         		pGlobal.gstPhotocopy:nNbBit = 16
         		
         	// Colors
         	CASE TwainRGB
         		pGlobal.gstPhotocopy:nColor = TwainRGB
         		pGlobal.gstPhotocopy:nResolution = LoadParameter(PARAM_RESOLUTION_COLOR, 150)
         		pGlobal.gstPhotocopy:nNbBit = 16
         END
         
         // Retrieve the selected lightness
         pGlobal.gstPhotocopy:rLightness = SLD_Lightness
         
         // Starts the photocopy
         pGlobal.Photocopying()
         
         // Actual photocopy
         MyWindow..ReturnedValue = True
         
         // Closes the window
         Close()
        type : 18
      -
